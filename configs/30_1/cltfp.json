        "out_dir": "./model/30_1/cltfp",
        "save_n_best": 3,
        "early_stopping_patience": 5,
        "early_stopping_delta": 1e-4
    },

    "model_config": {
        "in_timesteps": 30,
        "out_timesteps": 1,
        "out_cnn_channels": 50,
        "first_cnn_kernel": 10,
        "second_cnn_kernel": 10,
        "third_cnn_kernel": 10,
        "lstm_hidden_channels": 64
    },
    "optimizer_config": {
        "lr": 1e-3
    },
    "scheduler_config": {
        "decay_epoch": 3,
        "gamma": 0.7
    }
